<?xml version="1.0"?>
<odoo>
    <data>

        <!-- Scheduler to Populate Rem Unit Search -->
        <record forcecreate="True" id="ir_cron_populate_rem_unit_search" model="ir.cron">
            <field name="name">Populate Rem Unit Search</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model" eval="'rem.unit.search'"/>
            <field name="function" eval="'populate_rem_unit_search'"/>
        </record>

    </data>
</odoo>
